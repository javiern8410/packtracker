{
	"name": "packtracker-client",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "vitest --ui",
		"coverage": "vitest run --coverage",
		"lint": "eslint 'src/**/*.{js,ts,tsx}' --fix"
	},
	"lint-staged": {
		"src/**/*.{js,ts,tsx}": [
			"eslint --fix",
			"git add"
		],
		"*.{js,jsx,ts,tsx,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "5.6.0",
		"@typescript-eslint/parser": "5.6.0",
		"daisyui": "2.50.0",
		"jsdom": "21.1.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"swr": "2.0.3"
	},
	"devDependencies": {
		"@types/react": "18.0.27",
		"@types/react-dom": "18.0.10",
		"@vitejs/plugin-react": "3.1.0",
		"@vitest/ui": "0.28.4",
		"autoprefixer": "10.4.13",
		"eslint": "8.4.1",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-simple-import-sort": "7.0.0",
		"lint-staged": "12.1.2",
		"postcss": "8.4.21",
		"prettier": "2.5.1",
		"tailwindcss": "3.2.6",
		"typescript": "4.9.3",
		"vite": "4.1.0",
		"vitest": "0.28.4"
	}
}
